
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.tscp.mvne.customer.dao">

  <class name="com.tscp.mvne.customer.dao.CustAcctMapDAO">
    <id name="account_no" column="account_no" />
    <property name="cust_id" column="cust_id" />
  </class>

  <class name="com.tscp.mvne.customer.dao.GeneralSPResponse">
    <id name="mvnemsgcode" column="mvnemsgcode" />
    <property name="status" column="status" />
    <property name="mvnemsg" column="mvnemsg" />
  </class>

  <sql-query name="fetch_cust_acct_map" callable="true">
    <return class="com.tscp.mvne.customer.dao.CustAcctMapDAO">
      <return-property name="cust_id" column="cust_id" />
      <return-property name="account_no" column="account_no" />
    </return>
    { call MVNE_API_PKG.FETCH_CUST_ACCT_MAP(?,:in_cust_id) }
  </sql-query>

  <sql-query name="fetch_cust_from_acct" callable="true">
    <return class="com.tscp.mvne.customer.dao.CustAcctMapDAO">
      <return-property name="cust_id" column="cust_id" />
      <return-property name="account_no" column="account_no" />
    </return>
    { call MVNE_API_PKG.FETCH_CUST_FROM_ACCT(?,:in_account_no) }
  </sql-query>

  <sql-query name="ins_cust_acct_map" callable="true">
    <return class="com.tscp.mvne.customer.dao.GeneralSPResponse">
      <return-property name="status" column="status" />
      <return-property name="mvnemsg" column="mvnemsg" />
      <return-property name="mvnemsgcode" column="mvnemsgcode" />
    </return>
    { call MVNE_API_PKG.INS_CUST_ACCT_MAP(?, :cust_id, :account_no) }
  </sql-query>

  <sql-query name="del_cust_acct_map" callable="true">
    <return class="com.tscp.mvne.customer.dao.GeneralSPResponse">
      <return-property name="status" column="status" />
      <return-property name="mvnemsg" column="mvnemsg" />
      <return-property name="mvnemsgcode" column="mvnemsgcode" />
    </return>
    { call MVNE_API_PKG.DEL_CUST_ACCT_MAP(?, :cust_id, :account_no) }
  </sql-query>

</hibernate-mapping>
